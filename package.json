    "url": "git+https://github.com/JavaSmallSmallWhite/noomi-rpc-node"
  },
  "homepage": "https://gitee.com/javafirstfirst/noomi-rpc-node#readme",
  "dependencies": {
    "@furyjs/fury": "^0.0.18",
    "axios": "^1.6.5",
    "json5": "^2.2.3",
    "log4js": "^6.9.1",
    "nacos": "^2.6.0",
    "node-zookeeper-client": "^1.1.3",
    "zookeeper": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/node-zookeeper-client": "^0.2.13",
    "madge": "^6.1.0"
